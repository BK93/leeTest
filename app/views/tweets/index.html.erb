<div id="welcome">

<h1>twitter!!</h1>

<table class="table table-striped table-hover ">
   <% @tweets.each do |tweet| %>
    <tr class="active">
      <td><%= link_to tweet.content, tweet_path(tweet.id) %></td>
    </tr>
   <% end %>
   
   <% @retweets.each do |retweet| %>
    <tr class="warning">
      <td><%= retweet.content %></td>
    </tr>
   <% end %>
</table>

<%= link_to new_tweet_path, class: "btn btn-primary" do %>
    Tweet
<% end %>

</div>